{
  "name": "vue-cv-annotation",
  "version": "0.2.4",
  "description": "vue image annotation preview and edit tool, can be used for CV",
  "files": [
    "dist",
    "types"
  ],
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./types/index.d.ts",
  "style": "./dist/index.css",
  "private": false,
  "license": "MIT",
  "keywords": [
    "annotation",
    "cv",
    "vue",
    "tool"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },
    "./*": "./*"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "lint": "eslint --ext .ts,.vue src && stylelint **/*.vue",
    "preview": "vite preview"
  },
  "peerDependencies": {
    "vue": "^3.2.26"
  },
  "devDependencies": {
    "@types/node": "^16.11.12",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "@vitejs/plugin-vue": "^2.0.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.2.0",
    "postcss-html": "^1.3.0",
    "prettier": "^2.5.1",
    "stylelint": "^14.1.0",
    "stylelint-config-recommended-vue": "^1.1.0",
    "typescript": "^4.4.4",
    "vite": "^2.7.1",
    "vite-plugin-dts": "^0.9.6",
    "vue-tsc": "^0.29.8"
  },
  "stylelint": {
    "extends": "stylelint-config-recommended-vue"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "parser": "vue-eslint-parser",
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    },
    "plugins": [
      "@typescript-eslint",
      "prettier"
    ],
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "plugin:vue/vue3-recommended",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "@typescript-eslint/no-explicit-any": "off",
      "@typescript-eslint/no-non-null-assertion": "off",
      "@typescript-eslint/no-unused-vars": "off",
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "jsxBracketSameLine": true,
          "printWidth": 120
        }
      ]
    }
  },
  "author": "Djuvl",
  "homepage": "https://djuvl.github.io/vue-cv-annotation",
  "repository": "https://github.com/Djuvl/vue-cv-annotation.git"
}
